{
  "name": "react-flux-bootstrap",
  "version": "1.0.0",
  "dependencies": {},
  "description": "A starter template for an isomorphic web app, using Node, React + flux and SASS.",
  "homepage": "https://github.com/andyshora/react-flux-bootstrap",
  "repository": {
    "type": "git",
    "url": "https://github.com/andyshora/react-flux-bootstrap.git"
  },
  "scripts": {
    "dev": "webpack --config conf/webpack-dev.js | webpack-dev-server --port 8080 --config conf/webpack-dev.js --hot --history-api-fallback --progress --colors --inline --content-base ./build",
    "dev-server-client": "./node_modules/.bin/webpack --progress --watch --config conf/webpack-dev-server.js | ./node_modules/.bin/webpack-dev-server --port 8081 --config conf/webpack-dev-server.js --hot --history-api-fallback --progress --colors --inline --content-base ./build",
    "dev-server": "node build/server.js",
    "dev-server-debug": "node-debug --debug-brk build/server.js --debug --source-maps",
    "build": "./node_modules/.bin/webpack --config conf/webpack-prod.js --progress --profile --colors",
    "start": "node dist/server.js",
    "test": "./node_modules/.bin/mocha app/**/*-test.js --colors --compilers js:babel/register --recursive"
  },
  "devDependencies": {
    "babel-core": "^5.1.11",
    "babel-eslint": "^3.1.1",
    "babel-loader": "^5.1.4",
    "babel-runtime": "^5.1.11",
    "clean-webpack-plugin": "^0.1.2",
    "css-loader": "^0.10.1",
    "eslint": "^0.20.0",
    "eslint-loader": "^0.11.2",
    "eslint-plugin-react": "^2.2.0",
    "extract-text-webpack-plugin": "^0.6.0",
    "file-loader": "^0.8.1",
    "html-webpack-plugin": "^1.2.0",
    "jsdom": "^2.0.0",
    "jsx-loader": "^0.13.2",
    "mocha": "^2.1.0",
    "node-sass": "^3.1.2",
    "react-hot-loader": "^1.2.7",
    "react-router": "^0.13.2",
    "sass-loader": "1.0.2",
    "style-loader": "^0.10.2",
    "url-loader": "^0.5.5",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.8.2"
  },
  "dependencies": {
    "alt": "^0.16.10",
    "babel": "^5.1.11",
    "classnames": "^2.1.1",
    "express": "^4.12.3",
    "iso": "^4.1.0",
    "lodash": "^3.9.3",
    "marked": "^0.3.3",
    "node-fetch": "^1.2.1",
    "object-assign": "^2.0.0",
    "react": "^0.13.2",
    "serve-favicon": "^2.2.1",
    "whatwg-fetch": "^0.8.1"
  },
  "keywords": [
    "react",
    "flux",
    "isomorphic",
    "webpack",
    "kit",
    "express",
    "node"
  ],
  "license": "MIT"
}
